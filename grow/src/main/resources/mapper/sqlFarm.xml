<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.master.grow.farm.mapper.FarmMapper">

	<select id="CodeCheckEvent" resultType="int">
		select COUNT(code) from farm_data
		where code = #{farmCode}
	</select>

</mapper>